from django.db import models

# Create your models here.
class Profile(models.Model):
	nombre = models.CharField(max_length=200)
	apellidos = models.CharField(max_length=200, blank=True)
	nick = models.CharField(max_length=50, blank=True, unique=True)
	cita_favorita = models.CharField(blank=True)
	ciudad = models.CharField(max_length=70, blank=True)
	num = models.IntegerField()
	email = models.CharField(max_length=70, blank=True)
	web = models.CharField(max_length=70, blank=True)
